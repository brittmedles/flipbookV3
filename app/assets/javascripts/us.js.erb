$(window).load(function() {
  $(this).find('#build-with-us').fadeIn(800);
  $(this).find('#lead-by').fadeIn(800);
});

$(document).ready(function() {
  
  $(".icon-arrow-down1").click(function (){
    $('body').animate({
      scrollTop: $("#build-with-us").offset().top
      }, 800);
    return false;
  });
  
  $(".icon-arrow-down2").click(function (){
    $('body').animate({
      scrollTop: $("#login-footer").offset().top
      }, 800);
    return false;
  });
  
  $(".icon-arrow-up").click(function (){
    $('body').animate({
      scrollTop: $("#watch-us-build").offset().top
      }, 800);
    return false;
  });
});

